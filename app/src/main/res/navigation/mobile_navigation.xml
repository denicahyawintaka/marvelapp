<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.edottest.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_categories"
        android:name="com.example.edottest.ui.dummyscreen.CategoriesFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_dummy" />

    <fragment
        android:id="@+id/navigation_download"
        android:name="com.example.edottest.ui.dummyscreen.DownloadFragment"
        android:label="@string/title_download"
        tools:layout="@layout/fragment_dummy" />

    <fragment
        android:id="@+id/navigation_more"
        android:name="com.example.edottest.ui.dummyscreen.MoreFragment"
        android:label="@string/title_more"
        tools:layout="@layout/fragment_dummy" />
</navigation>